<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:layout="sap.ui.layout">
	<Dialog title="Draft Record">
		<content>
			<Text class="sapUiLargeMargin" text="{i18n>contasdraftmsg}"/>
		</content>
		<beginButton>
			<Button id="contAsDraftBtn" type="Emphasized" text="Continue as draft" press="openClaimWizard"/>
		</beginButton>
		<endButton>
			<Button id="startNewClaimBtn" type="Emphasized" text="Start new claim" press="openInjuryTab"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>