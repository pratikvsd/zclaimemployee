<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:layout="sap.ui.layout">
	<Dialog id="PrivacyDialog" showHeader="false" verticalScrolling="false" contentHeight="90%" contentWidth="90%">
		<content>
		<f:DynamicPage stickySubheaderProvider="claimFormWizard" toggleHeaderOnTitleClick="false" class="sapUiNoContentPadding" showFooter="true">
			<f:content>
				<Text class="sapUiLargeMargin" text="{i18n>PrivacyStatement}"/>
			</f:content>
			<f:footer>
				<OverflowToolbar>
					<Button text="{i18n>WizardFooterCancelBtn}" type="Reject" press="handleWizardCancel"/>
					<ToolbarSpacer/>
					<Button text="{i18n>PrivacyStatementAcceptBtn}" type="Accept"  press="openClaimWizard"/>
				</OverflowToolbar>
			</f:footer>
		</f:DynamicPage>
		</content>
	</Dialog>
</core:FragmentDefinition>