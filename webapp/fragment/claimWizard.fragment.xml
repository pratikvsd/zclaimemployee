<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:layout="sap.ui.layout">
	<Dialog id="wizardDialog" showHeader="false" verticalScrolling="false" contentHeight="90%" contentWidth="90%">
		<content>
			<f:DynamicPage stickySubheaderProvider="claimFormWizard" toggleHeaderOnTitleClick="false" class="sapUiNoContentPadding" showFooter="true">
				<f:content>
					<Wizard id="claimFormWizard" showNextButton="false">
						<WizardStep id="personalDetailStep" title="{i18n>claimFormWizardPersonalDetails}">
							<VBox class="sapUiSmallMargin">
								<form:SimpleForm id="SimpleFormChange354wideDual" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3"
									labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2"
									columnsL="1" columnsM="1" singleContainerFullSize="false">
									<form:content>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Title}"/>
										<Input id="userTitle"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_FamilyName}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_GivenName}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_DOB}"/>
										<DatePicker id="claimFormBirthDate"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Gender}"/>
										<Select>
											<items>
												<core:Item text="Male"/>
												<core:Item text="Female"/>
												<core:Item text="Other"/>
											</items>
										</Select>
										<Label text="{i18n>WizardOtherknownorpreviouslegalnamesegMaidenname}"/>
										<Input/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Address}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Suburb}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_State}"/>
										<Select>
											<items>
												<core:Item text=""/>
												<core:Item text=""/>
												<core:Item text=""/>
											</items>
										</Select>
										<Label text="{i18n>WizardPersonalDetailFormLabel_PAFC}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Postcode}"/>
										<Input type="Text"/>
										<core:Title text="{i18n>WizardPersonalDetailFormContactDetails}" level="H3"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Mobile}"/>
										<Input type="Number"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Work}"/>
										<Input type="Number"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Home}"/>
										<Input type="Number"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_Email}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_question1}"/>
										<Input type="Text"/>
										<Label text="{i18n>WizardPersonalDetailFormLabel_question2}"/>
										<Select>
											<items>
												<core:Item text=""/>
												<core:Item text=""/>
												<core:Item text=""/>
											</items>
										</Select>
										<Label text="{i18n>WizardPersonalDetailFormLabel_question3}"/>
										<Select>
											<items>
												<core:Item text=""/>
												<core:Item text=""/>
												<core:Item text=""/>
											</items>
										</Select>
									</form:content>
								</form:SimpleForm>
							</VBox>
						</WizardStep> 
						<WizardStep id="injuryDetailStep" title="{i18n>claimFormWizardInjuryDetails}"></WizardStep>
						<WizardStep id="employmentDetailStep" title="{i18n>claimFormWizardEmploymentDetails}"/>
						<WizardStep id="workerEarningStep" title="{i18n>claimFormWizardWorkersprimaryearning}"/>
						<WizardStep id="returntoWorkStep" title="{i18n>claimFormWizardReturntowork}"/>
						<WizardStep id="workerDecStep" title="{i18n>claimFormWizardWorkerdeclaration}"/>
						<WizardStep id="attachmentStep" title="{i18n>claimFormWizardAttachments}"/>
					</Wizard>
				</f:content>
				<f:footer>
					<OverflowToolbar>
						<Button text="{i18n>WizardFooterCancelBtn}" type="Reject" press="handleWizardCancel"/>
						<ToolbarSpacer/>
						<Button text="{i18n>WizardFooterPreviousBtn}" icon="sap-icon://navigation-left-arrow" type="Emphasized" visible="{/backButtonVisible}"
							press="onDialogBackButton"/>
						<Button text="{i18n>WizardFooterNextBtn}" icon="sap-icon://navigation-right-arrow" type="Emphasized" enabled="{/nextButtonEnabled}"
							press="onDialogNextButton"/>
						<MenuButton type="Accept" text="{i18n>WizardFooterSaveBtn}" icon="sap-icon://save">
							<menu>
								<Menu>
									<MenuItem text="{i18n>WizardFooterSubmitBtn}" icon="sap-icon://accept" press="onPress"/>
									<MenuItem text="{i18n>WizardFooterSaveasdraftBtn}" icon="sap-icon://open-folder" press="onPress"/>
								</Menu>
							</menu>
						</MenuButton>
						<!--<Button text="Save" type="Emphasized" visible="{/finishButtonVisible}" press="handleWizardSubmit"/>-->
					</OverflowToolbar>
				</f:footer>
			</f:DynamicPage>
		</content>
	</Dialog>
</core:FragmentDefinition>